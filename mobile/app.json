{
  "expo": {
    "name": "PMS Ops",
    "slug": "pms-ops",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3f3f41"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pms.ops",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pms.ops",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "apiUrl": "https://pms-ops-backend.onrender.com/api",
      "env": "production",
      "eas": {
        "projectId": "90151316-fcf8-4af1-a451-73375b597e69"
      }
    }
  }
}
